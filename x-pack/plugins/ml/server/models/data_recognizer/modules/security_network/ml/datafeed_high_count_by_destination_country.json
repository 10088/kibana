{
  "job_id": "high-count-by-destination-country",
  "indices": [
    "logs-*",
    "filebeat-*",
    "packetbeat-*"
  ],
  "max_empty_searches": 10,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "event.category": "network"
          }
        },
        {
          "exists": {
            "field": "destination.geo.country_name"
          }
        }
      ]
    }
  }
}
